<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yintu.rixing.data.DataArchivesCollectionMapper">
    <insert id="insertSelective" parameterType="com.yintu.rixing.data.DataCommonAll">
        insert into `${tableName}`
        <trim prefix="(" suffix=")">
            <foreach collection="dataCommons" index="index" item="dataCommon" separator="," close=",">
                <if test="dataCommon.fieldValue != null">
                    `${dataCommon.fieldName}`
                </if>
            </foreach>
            `status`
        </trim>
        <trim prefix="values (" suffix=")">
            <foreach collection="dataCommons" index="index" item="dataCommon" separator="," close=",">
                <if test="dataCommon.fieldValue != null">
                    #{dataCommon.fieldValue}
                </if>
            </foreach>
            1
        </trim>
    </insert>

    <insert id="insertSelectiveBatch" parameterType="com.yintu.rixing.data.DataCommonAll">
        insert into `${tableName}`
        <trim prefix="(" suffix=")">
            <foreach collection="lists" index="index" item="list" separator="," close=",">
                <if test="index==1">
                    <if test="list.index.dataCommon.fieldValue != null">
                        `${dataCommon.fieldName}`
                    </if>
                </if>
            </foreach>
            `status`
        </trim>
        values
        <foreach collection="lists" index="index" item="dataCommons" close=",">
            <trim prefix="(" suffix=")">
                <foreach collection="dataCommons" index="index" item="dataCommon" separator="," close=",">
                    <if test="dataCommon.fieldValue != null">
                        #{dataCommon.fieldValue}
                    </if>
                </foreach>
                1
            </trim>
        </foreach>
    </insert>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete
        from `${tableName}`
        where id = #{id,jdbcType=INTEGER}
    </delete>

    <delete id="deleteByPrimaryKeys" parameterType="java.lang.Integer">
        delete
        from `${tableName}`
        where id in
        <foreach collection="ids" item="id" open="(" close=")" separator=",">
            #{id,jdbcType=INTEGER}
        </foreach>
    </delete>
    <update id="updateByPrimaryKeySelective" parameterType="com.yintu.rixing.data.DataCommonAll">
        update `${tableName}`
        <set>
            <foreach collection="dataCommons" index="index" item="dataCommon" separator=",">
                <if test="dataCommon.fieldValue != null">
                    `${dataCommon.fieldName}` = #{dataCommon.fieldValue}
                </if>
            </foreach>
        </set>
        where id = #{id,jdbcType=INTEGER}
    </update>

    <select id="selectByPrimaryKey" resultType="java.util.Map">
        select *
        from `${tableName}`
        where id = #{id,jdbcType=INTEGER}
    </select>
    <select id="selectPage" resultType="java.util.Map">
        select *
        from `${tableName}`
        where `status` = 1
        order by `id` DESC
    </select>
</mapper>
